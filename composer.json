{
    "name": "qdequippe/mailjet-api-php",
    "description": "PHP client to interact with Mailjet API",
    "type": "library",
    "keywords": ["mailjet", "api", "sdk", "email"],
    "homepage": "https://github.com/qdequippe/mailjet-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Dequippe",
            "email": "quentin@dequippe.tech",
            "homepage": "https://dequippe.tech"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "ramsey/uuid": "^4.1",
        "symfony/http-client": "^4.4.16 || ^5.1.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Qdequippe\\Mailjet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qdequippe\\Mailjet\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
